{
    "name": "io",
    "displayName": "io",
    "description": "io language syntax highlighting tool.",
    "scripts": {
        "compile": "gulp compile-extension:io",
        "watch": "gulp watch-extension:io"
    },
    "keywords": [
        "languages",
        "syntax highlighting",
        "vscode",
        "io",
        "iolang",
        "extension"
    ],
    "version": "0.0.1",
    "publisher": "kennethceyer",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "io",
            "aliases": ["io"],
            "extensions": [".io"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "io",
            "scopeName": "source.io",
            "path": "./syntaxes/io.tmLanguage.json"
        }]
    }
}